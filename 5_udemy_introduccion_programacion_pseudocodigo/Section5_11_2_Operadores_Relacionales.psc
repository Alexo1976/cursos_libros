Proceso Operadores_Relacionales
	// Operadores relacionales/comparativos
	DEFINIR A, B COMO ENTEROS;
	A <- 5;
	B <- 2;
	
	ESCRIBIR A = B;
	ESCRIBIR A <> B;
	ESCRIBIR A < B;
	ESCRIBIR A > B;
	ESCRIBIR A <= B;
	ESCRIBIR A >= B;
	
	ESCRIBIR VERDADERO = VERDADERO;
	ESCRIBIR FALSO <> VERDADERO;
	
	ESCRIBIR 'A' = 'B';
	ESCRIBIR 'A' <> 'B';
FinProceso
